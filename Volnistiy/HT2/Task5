
#include <iostream>
using namespace std;

int main()
{
	int a, b, s, i;


	cout << "Delimoe: ";
	cin>> a;

	while (a <= 0)
	{
		cout << "oshibka  ";
		cin >>  a;
	}

	cout << "Delitel': ";
	cin >> b;

	while (b <= 0)
	{
		cout << "oshibka  ";
		cin >> b;
	}

	i = a;
	s = 0;

	while (i > 0 && i >= b)
	{
		i -= b;
		s++;
	}

	cout << "4astnoe: " << s << endl;
	cout << "Ostatok: " << i << endl;
}
